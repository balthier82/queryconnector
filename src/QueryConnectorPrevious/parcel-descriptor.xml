<parcel language="Java" xmlns:parcel="scripting.dtd">
    <script language="Java">
		<locale lang="en">
            <displayname value="QueryConnector"/>
            <description>
                Starts the query connector wizard
            </description>
        </locale>
        <locale lang="it">
            <displayname value="QueryConnector"/>
            <description>
                Avvia il processo di aggancio di una query al foglio di calcolo
            </description>
        </locale>
        <functionname value="QueryConnector.attach"/>
        <logicalname value="QueryConnector.attach"/>
        <languagedepprops>
          <prop name="classpath" value="queryconnector.jar:LODBConnector.jar:JSONLibrary.jar"/>
        </languagedepprops>
    </script>
	<script language="Java">
		<locale lang="en">
            <displayname value="QueryConnector"/>
            <description>
				Update data of an attached query
            </description>
        </locale>
        <locale lang="it">
            <displayname value="QueryConnector"/>
            <description>
                Aggiorna i dati di una query gia' agganciata al foglio di calcolo
            </description>
        </locale>
        <functionname value="QueryConnector.update"/>
        <logicalname value="QueryConnector.update"/>
        <languagedepprops>
          <prop name="classpath" value="queryconnector.jar:LODBConnector.jar:JSONLibrary.jar"/>
        </languagedepprops>
    </script>
	<script language="Java">
		<locale lang="en">
            <displayname value="QueryConnector"/>
            <description>
				Update data of all attached query
            </description>
        </locale>
        <locale lang="it">
            <displayname value="QueryConnector"/>
            <description>
                Aggiorna i dati di tutte le query agganciate al foglio di calcolo
