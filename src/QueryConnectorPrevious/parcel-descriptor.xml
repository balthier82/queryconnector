        </languagedepprops>
    </script>
	<script language="Java">
		<locale lang="en">
            <displayname value="QueryConnector"/>
            <description>
				Show connection info
            </description>
        </locale>
        <locale lang="it">
            <displayname value="QueryConnector"/>
            <description>
                Mostra le informazioni di connessione al db
            </description>
        </locale>
        <functionname value="QueryConnector.info"/>
        <logicalname value="QueryConnector.info"/>
        <languagedepprops>
          <prop name="classpath" value="queryconnector.jar:LODBConnector.jar:JSONLibrary.jar"/>
        </languagedepprops>
    </script>
